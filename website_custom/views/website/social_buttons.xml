<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <data>
        <template id="social_botton" inherit_id="ks_theme_kinetik.custom_footer_scroll_top">
            <xpath expr="//section[@class='ks-scroll-top flex-center']" position="before">
                <script type="text/javascript" src="/website_custom/static/src/js/menu_collapse.js"/>
                <t t-set="e" t-value="request.env['menu.social.link'].sudo().search([])"/>
                <div class="active">
                    <div class="menu-social-link content-menu bg-primary">
                        <t t-foreach="e" t-as="o">
                            <div class="box-item">
                                <a t-att-href="o.link" class="btn btn-primary social-botton-menu flex-center" type="button">
                                    <i t-att-class="o.icon" />
                                </a>
                            </div>
                        </t>
                        <div class="box-item">
                            <button id="click2" class="btn btn-primary social-botton-menu flex-center" type="button">
                                <i class="fa fa-times"/>
                            </button>
                        </div>
                    </div>
                    <button id="click" class="btn btn-primary social-botton flex-center hola" type="button">
                        <i class="fa fa-inbox"/>
                    </button>
                </div>
            </xpath>
        </template>
    </data>
</odoo>