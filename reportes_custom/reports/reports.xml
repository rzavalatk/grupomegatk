		<record id="account_factura_custom" model="ir.actions.report">
			<field name="name">Factura</field>
			<field name="model">account.move</field>
			<field name="report_name">reportes_custom.factura_custom_print_view</field>
			<field name="report_type">qweb-pdf</field>
			<field name="binding_model_id" ref="account.model_account_move"/>
			<field name="binding_type">report</field>
			<field name="paperformat_id" ref="formatos_papel_mega.report_paperformat_ducumentos"/>
			<field name="attachment_use">True</field>
			<field name="print_report_name">'Factura - %s' % (object.name)</field>
		</record>

