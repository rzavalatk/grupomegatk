
<odoo>

    <data>
        <record id="sequence_session" model="ir.sequence">
            <field name="name">Sorteo Ticket Sequence</field>
            <field name="code">ticket.sequence</field>
            <field name="active">true</field>
            <field name="prefix">Ticket/</field> <!-- Se Puede personalizar el prefijo según necesidades -->
            <field name="padding">3</field> <!-- Se Puede ajustar el relleno según necesidades -->
