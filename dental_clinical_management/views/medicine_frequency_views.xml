<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!--  Tree View-->
    <record id="medicine_frequency_view_tree" model="ir.ui.view">
            <field name="name">medicine.frequency.view.tree</field>
            <field name="model">medicine.frequency</field>
            <field name="arch" type="xml">
                <tree string="Frequency" editable="bottom">
                    <field name="code" required="True"/>
                    <field name="medicament_frequency" required="True"/>
                </tree>
            </field>
    </record>
</odoo>
